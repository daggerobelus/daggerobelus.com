  "notable_quotes": [
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "quelle s'avoit ja chaussé avec une sorciere, et qu'elle s'y chaussoit encore",
      "english": "that she had already quarreled with a witch, and that she quarreled with her still",
      "context": "Heard at Claudon Ozelle's house, reported by witness 3"
    },
    {
      "speaker": "someone to Mengeotte",
      "french": "Tante Mengeotte faictes tout doucement",
      "english": "Aunt Mengeotte do it very gently",
      "context": "When she was working by river"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "qu'elle n'estoit tante Mengeotte, mais qu'elle estoit tante Genoxe",
      "english": "that she was not Aunt Mengeotte, but that she was Aunt Genoxe",
      "context": "Her reply when called Tante Mengeotte"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "qu'on en parloit tout communement et haultement",
      "english": "that everyone spoke of it commonly and loudly",
      "context": "Acknowledging that people were calling her a witch"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "estoit bien aisé que son coeur volloit en l'air",
      "english": "it was quite easy that her heart flew in the air",
      "context": "When lamenting being called witch, reported by witness 5"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "qu'il y avoit encore une qui y avoit aydé",
      "english": "that there was still another who had helped",
      "context": "To witness 6 about illness of child, suggesting she herself was also involved"
    },
    {
      "speaker": "Gabriel Ozelle (son-in-law)",
      "french": "une vraye sorciere",
      "english": "a true witch",
      "context": "What Gabriel Ozelle called her, reported by witness 8"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "qu'elle puisse estre bruslée sy elle l'estoit",
      "english": "may she be burned if she were one",
      "context": "Her reply to being called a witch by her son-in-law"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "qu'elle l'auroit bien tost",
      "english": "that she would have him soon",
      "context": "Threat to grandson Nicolas, age 4, reported by witness 9"
    },
    {
      "speaker": "Mengeotte veuve Anthoine Durand",
      "french": "que c'estoit grand dommage, qu'il falloit bien maudire les sorciers et sorcieres, qu'il les falloit tous brusler et que sa fille (pour lors malade) estoit ensorcelée",
      "english": "that it was a great pity, that one must curse witches, that they must all be burned and that her daughter (then sick) was bewitched",
      "context": "Speaking about witchcraft while her own daughter was ill, reported by witness 13"
